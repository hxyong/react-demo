    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "_dev": "webpack-dev-server --open --hot",
    "_prod": "webpack-dev-server",
    "_build": "webpack --progress --hide-modules ",
    "dev1": "cross-env NODE_ENV=development FLAVOR=Default npm run _dev",
    "prod1": "cross-env NODE_ENV=production FLAVOR=Production npm run _prod",
    "watch": "cross-env NODE_ENV=development FLAVOR=Default npm run _watch",
    "analyze": "cross-env ANALYZE=true npm run build",
    "build-dev1": "cross-env-shell NODE_ENV=development FLAVOR=Default \"npm run _build && gulp\"",
    "build1": "cross-env-shell NODE_ENV=production FLAVOR=Production \"npm run _build \""
  },
  "devDependencies": {
    "@types/react": "^16.4.7",
